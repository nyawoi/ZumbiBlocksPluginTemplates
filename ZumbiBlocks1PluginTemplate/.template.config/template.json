{
	"$schema": "http://json.schemastore.org/template",
	"identity": "ZumbiBlocks1PluginTemplate",
	"name": "Zumbi Blocks 1 Plugin",
	"shortName": "zb1-plugin",
	"author": "wowi",
	"preferNameDirectory": true,
	"classifications": [
		"Zumbi Blocks 1",
		"BepInEx 5",
		"Plugin"
	],
	"tags": {
		"language": "C#",
		"type": "project"
	},
	"sourceName": "ExamplePlugin",
	"sources": [
		{
			"source": "./",
			"target": "./",
			"include": "**/*",
			"exclude": [
				"**/[Bb]in/**",
				"**/[Oo]bj/**",
				"**/[Dd]ebug/**",
				"**/[Rr]elease/**",
				".template.config/**/*",
				".vs/**/*",
				".vscode/**/*",
				".idea/**/*"
			]
		}
	],
	"symbols": {
		"Author": {
			"type": "parameter",
			"datatype": "text",
			"description": "Your name",
			"replaces": "AuthorName",
			"defaultValue": "AuthorName"
		},
		"Framework": {
			"type": "parameter",
			"datatype": "choice",
			"description": "The target framework for the project",
			"replaces": "net472",
			"defaultValue": "net472",
			"enableQuotelessLiterals": true,
			"choices": [
				{
					"choice": "net462",
					"description": ".NET Framework 4.6.2"
				},
				{
					"choice": "net47",
					"description": ".NET Framework 4.7"
				},
				{
					"choice": "net471",
					"description": ".NET Framework 4.7.1"
				},
				{
					"choice": "net472",
					"description": ".NET Framework 4.7.2"
				},
				{
					"choice": "net48",
					"description": ".NET Framework 4.8"
				},
				{
					"choice": "net481",
					"description": ".NET Framework 4.8.1"
				}
			]
		}
	}
}
